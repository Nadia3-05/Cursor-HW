<!-- <script type="rocketlazyloadscript">
jQuery(document).ready(function($) {
    var time = 2,
        cc = 1;
    $(window).scroll(function() {
        $('').each(function() {
            var
                cPos = $(this).offset().top,
                topWindow = $(window).scrollTop();
            if (cPos < topWindow + 200) {
                if (cc < 2) {
                    $(".numbers-item__number").addClass("viz");
                    $('div').each(function() {
                        var
                            i = 1,
                            num = $(this).data('num'),
                            step = 1000 * time / num,
                            that = $(this),
                            int = setInterval(function() {
                                if (i <= num) {
                                    that.html(i);
                                } else {
                                    cc = cc + 100;
                                    clearInterval(int);
                                }
                                i++;
                            }, step);
                    });
                }
            }
        });
    });
});
</script> -->

