<script type="rocketlazyloadscript">window.addEventListener('DOMContentLoaded', function() {
//   service-block
jQuery(document).ready(function($) {
    function sameHeight(elem) {
        let maxHeight = 0;
        $(elem).each(function() {
            if ($(this).height() > maxHeight) {
                maxHeight = $(this).height();
            }
        });
        $(elem).height(maxHeight);
    }

    if ($(window).width() > 588) {
        sameHeight($(".service-item__header"));
    } else {
        $(function() {
            $(".service-item__header").on("click", function(e) {
                e.preventDefault();
                let $this = $(this);
                if (!$this.hasClass("service-item__active")) {
                    $(".service-list").slideUp(800);
                    $(".service-item__header").removeClass("service-item__active");
                }
                $this.toggleClass("service-item__active");
                $this.next().slideToggle();
            });
        });
    }
});
// service-block
});</script>

